{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**", ".next/**", ".docusaurus/**"]
    },
    "client#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": [
        "BUNDLE_ANALYZE",
        "NEXT_PUBLIC_ALCHEMY_ID",
        "NEXT_PUBLIC_AXIOM_INGEST_ENDPOINT",
        "NEXT_PUBLIC_CDN_ENDPOINT",
        "NEXT_PUBLIC_IPFS_AUTH",
        "NEXT_PUBLIC_IPFS_ENDPOINT",
        "NEXT_PUBLIC_IPFS_GATEWAY",
        "NEXTAUTH_SECRET",
        "NODE_ENV",
        "S3_ACCESS_KEY_ID",
        "S3_BUCKET",
        "S3_ENDPOINT",
        "S3_REGION",
        "S3_SECRET",
        "VERCEL_URL"
      ]
    },
    "host#build": {
      "dependsOn": ["^build"],
      "outputs": [],
      "env": ["PORT", "SSL_CERT", "SSL_KEY"]
    },
    "lint": {
      "outputs": []
    },
    "deploy": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
