{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "lint": {},
    "build": {
      "dependsOn": ["^build", "codegen"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:docker": {
      "dependsOn": ["^build", "codegen"]
    },
    "codegen": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build", "codegen"],
      "cache": false
    }
  }
}
