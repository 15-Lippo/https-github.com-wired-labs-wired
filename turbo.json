{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "codegen"],
      "outputs": ["dist/**", ".next/**"]
    },
    "client#build": {
      "dependsOn": [
        "^build",
        "$NEXT_PUBLIC_AXIOM_INGEST_ENDPOINT",
        "$NEXT_PUBLIC_IPFS_ENDPOINT",
        "$NEXT_PUBLIC_IPFS_GATEWAY",
        "$NEXT_PUBLIC_IPFS_AUTH",
        "$NEXT_AUTH_SECRET",
        "$ALCHEMY_ID",
        "$S3_IMAGE_HOSTNAME",
        "$S3_ENDPOINT",
        "$S3_REGION",
        "$S3_ACCESS_KEY_ID",
        "$S3_SECRET",
        "$BUNDLE_ANALYZE",
        "$NODE_ENV",
        "$VERCEL_URL"
      ],
      "outputs": [".next/**"]
    },
    "codegen": {},
    "client#codegen": { "dependsOn": ["$DATABASE_URL"] },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["**/*.tsx", "**/*.ts"]
    },
    "lint": { "outputs": [] },
    "deploy": { "outputs": [] },
    "dev": { "cache": false }
  }
}
