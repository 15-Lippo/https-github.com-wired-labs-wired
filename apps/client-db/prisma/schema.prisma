generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model Project {
  id Int @id @default(autoincrement())

  owner String

  name        String?
  description String?
  image       String?

  scene       String?
  studioState String?

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
