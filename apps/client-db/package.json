{
  "name": "@wired-xr/client-db",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "codegen": "prisma generate",
    "dev": "yarn dev:prune && yarn dev:build && yarn dev:up",
    "dev:build": "docker compose -f docker-compose.dev.yml build",
    "dev:down": "docker compose -f docker-compose.dev.yml down",
    "dev:prune": "yarn dev:down && docker image rm -f client-db-api-dev",
    "dev:up": "docker compose -f docker-compose.dev.yml up",
    "docker:dev": "nodemon -L --exec \"yarn migrate:dev && yarn codegen && yarn build && yarn docker:start\"",
    "docker:start": "yarn migrate:dev && yarn codegen && yarn node --experimental-specifier-resolution=node ./build/server.js",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "start": "yarn start:prune && yarn start:build && yarn start:up",
    "start:build": "docker compose -f docker-compose.yml build",
    "start:down": "docker compose -f docker-compose.yml down",
    "start:prune": "yarn start:down && docker image rm -f client-db-api",
    "start:up": "docker compose -f docker-compose.yml up"
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "@trpc/server": "^9.25.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.0",
    "@wired-xr/tsconfig": "*",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.18",
    "prisma": "^4.1.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
